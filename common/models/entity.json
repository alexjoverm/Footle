{
  "name": "entity",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "user"
    },
    "establishments": {
      "type": "hasMany",
      "model": "establishment"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "entityMemberAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "entityMember",
      "permission": "ALLOW",
      "property": "__get__establishments"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
